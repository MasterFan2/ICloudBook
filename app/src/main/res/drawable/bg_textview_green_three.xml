<?xml version="1.0" encoding= "UTF-8"?>
<selector android:constantSize ="true"
    xmlns:android= "http://schemas.android.com/apk/res/android" >
    <item android:state_pressed= "true">
        <shape android:shape= "rectangle">
            <gradient android:startColor="@color/base_green_checked" android:endColor="@color/base_green_checked" android:angle="270.0" />
            <!--             <stroke android:width="1.0px" android:color="@color/more_pressed_end" /> -->
            <corners android:bottomLeftRadius= "3.0dip" android:bottomRightRadius ="3.0dip" android:topLeftRadius="3.0dip" android:topRightRadius="3.0dip" />

        </shape>
    </item>


    <item>
        <shape android:shape= "rectangle">
            <gradient android:startColor="@color/base_green" android:centerColor="@color/base_green" android:endColor="@color/base_green" android:angle="270.0" />
            <!--             <stroke android:width="1.0px" android:color="@color/more_pressed_end" /> -->
            <corners android:bottomLeftRadius= "3.0dip" android:bottomRightRadius ="3.0dip" android:topLeftRadius="3.0dip" android:topRightRadius="3.0dip" />

        </shape>
    </item>
</selector>
